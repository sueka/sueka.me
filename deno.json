{
  "imports": {
    "@b-fuze/deno-dom": "jsr:@b-fuze/deno-dom@^0.1.48",
    "@primer/octicons": "npm:@primer/octicons@^19.14.0",
    "@std/assert": "jsr:@std/assert@^0.204.0",
    "@std/yaml": "jsr:@std/yaml@^0.204.0",
    "css-has-pseudo": "npm:css-has-pseudo@^6.0.5",
    "date-fns": "npm:date-fns@^2.30.0",
    "lume/": "https://deno.land/x/lume@v2.0.3/",
    "markdown-it-abbr": "npm:markdown-it-abbr@^1.0.4",
    "markdown-it-anchor": "npm:markdown-it-anchor@^8.6.7",
    "markdown-it-attrs": "npm:markdown-it-attrs@^4.3.1",
    "markdown-it-bracketed-spans": "npm:markdown-it-bracketed-spans@^1.0.1",
    "markdown-it-collapsible": "npm:markdown-it-collapsible@^1.0.0",
    "markdown-it-deflist": "npm:markdown-it-deflist@^2.1.0",
    "markdown-it-emoji": "npm:markdown-it-emoji@^3.0.0",
    "markdown-it-footnote": "npm:markdown-it-footnote@^3.0.3",
    "markdown-it-mark": "npm:markdown-it-mark@^3.0.1",
    "markdown-it-multimd-table": "npm:markdown-it-multimd-table@^4.2.3",
    "markdown-it-named-code-blocks": "npm:markdown-it-named-code-blocks@^0.2.0",
    "markdown-it-ruby": "npm:markdown-it-ruby@^0.1.1",
    "postcss-custom-selectors": "npm:postcss-custom-selectors@^7.1.12",
    "postcss-extend-rule": "npm:postcss-extend-rule@^4.0.0",
    "truncate-html": "npm:truncate-html@^1.1.2"
  },
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run --unstable -A -",
    "build": "deno task lume",
    "serve": "deno task lume -s"
  },
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ]
  }
}
